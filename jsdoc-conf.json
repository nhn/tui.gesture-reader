{
  "source": {
    "include": [
      "src/js",
      "README.md"
    ],
    "exclude": [],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "name": "Gesture Reader",
    "logo": {
      "url": "https://cloud.githubusercontent.com/assets/12269563/20029815/01133928-a39a-11e6-80f3-12500a91c755.png",
      "width": "150px"
    }
  },
  "opts": {
    "recurse": true,
    "destination": "doc",
    "tutorials": "samples",
    "template": "node_modules/tui-doc-template",
    "package": "package.json"
  }
}
