{
  "source": {
    "include": [
      "src/js",
      "README.md"
    ],
    "exclude": [],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "name": "Gesture Reader"
  },
  "opts": {
    "recurse": true,
    "destination": "doc",
    "tutorials": "samples",
    "template": "node_modules/tui-doc-template",
    "package": "package.json"
  }
}
